<h1>Here are all the songs available:</h1>

<% @songs.each do |song| %>

  <%= link_to song.artist_name, artist_path(song.artist) %>
  -
  <%= link_to song.title, song_path(song) %><br>

<% end  %>


<h1>Songs</h1>

<ul>
  <% @songs.each do |song| %>
    <li>
      <a href="<%= song_path(song) %>">
        <%= song.artist_name %>
        -
        <%= song.title %>
      </a>
    </li>
  <% end %>
</ul>




